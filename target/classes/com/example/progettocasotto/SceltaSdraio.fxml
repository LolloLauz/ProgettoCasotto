<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.progettocasotto.SceltaSdraioView"
            prefHeight="400.0" prefWidth="600.0">

    <DatePicker fx:id="dataInizio" layoutX="74.0" layoutY="83.0" />
    <Label layoutX="107.0" layoutY="52.0" text="Seleziona data Inizio" />
    <Label layoutX="110.0" layoutY="130.0" text="Seleziona data Fine" />
    <DatePicker fx:id="dataFine" layoutX="74.0" layoutY="168.0" />
    <Button fx:id="mostraSdraioLiberi" layoutX="90.0" layoutY="232.0" mnemonicParsing="false" onAction="#handleMostraSdraioLiberi" text="mostra sdriao liberi" />
    <TextField fx:id="sdraio" layoutX="327.0" layoutY="83.0" promptText="inserisci il numero di sdraio che vuoi prenotare" />
    <Label fx:id="displaySdraioLiberi" layoutX="339.0" layoutY="235.0" prefHeight="130.0" prefWidth="210.0" />
    <Button fx:id="selectSdraio" layoutX="376.0" layoutY="126.0" mnemonicParsing="false" onAction="#handleSdraioSelezionato" text="inserisci numero sdraio" />
    <Button fx:id="confermaPrenotazione" layoutX="74.0" layoutY="340.0" mnemonicParsing="false" onAction="#handleConfermaPrenotazione" text="ConfermaPrenotazione" />
</AnchorPane>
