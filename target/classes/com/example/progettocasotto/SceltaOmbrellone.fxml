<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.progettocasotto.SelectOmbrellone">
   <children>
      <DatePicker fx:id="dataInizio" layoutX="74.0" layoutY="83.0" />
      <Label layoutX="107.0" layoutY="52.0" text="Seleziona data Inizio" />
      <Label layoutX="110.0" layoutY="130.0" text="Seleziona data Fine" />
      <DatePicker fx:id="dataFine" layoutX="74.0" layoutY="168.0" />
      <Button fx:id="ombrelloneSelezionato" layoutX="90.0" layoutY="232.0" mnemonicParsing="false" onAction="#handleMostraOmbrelloniLiberi" text="mostraOmbrelloniLiberi" />
      <TextField fx:id="ombrellone" layoutX="327.0" layoutY="83.0" promptText="inserisci l'ombrellone" />
      <Label fx:id="displayOmbrelloniLiberi" layoutX="339.0" layoutY="235.0" prefHeight="130.0" prefWidth="210.0" />
      <Button fx:id="selectOmbrellone" layoutX="376.0" layoutY="126.0" mnemonicParsing="false" onAction="#handleOmbrelloneSelezionato" text="selezionaOmbrellone" />
      <Button fx:id="confermaPrenotazione" layoutX="74.0" layoutY="340.0" mnemonicParsing="false" onAction="#handleConfermaPrenotazione" text="ConfermaPrenotazione" />

   </children>
</AnchorPane>
